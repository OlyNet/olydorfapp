<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/NavDrawerRowLayout"
    android:layout_width="match_parent"
    android:layout_height="@dimen/navigation_drawer_item_height">

    <eu.olynet.olydorfapp.customViews.TintOnStateImageView
        android:id="@+id/navDrawerItemIcon"
        style="@style/NavDrawerRowItemIcon"
        android:src="@drawable/ic_home_white_24dp"
        tools:ignore="ContentDescription"
        tools:tint="@color/nav_drawer_item_icon_selected" />

    <TextView
        android:id="@+id/navDrawerItemText"
        style="@style/NavDrawerRowItemText"
        tools:fontFamily="sans-serif-medium"
        tools:textColor="@color/nav_drawer_item_text_selected" />

</FrameLayout>
